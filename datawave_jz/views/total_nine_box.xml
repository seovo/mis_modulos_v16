<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="total_nine_box_tree" model="ir.ui.view">
            <field name="name">total.nine.box.tree</field>
            <field name="model">total.nine.box</field>
            <field name="arch" type="xml">
                <tree string="Service"     >
                    <field name="id_sql"  />
                    <field name="sku"  />
                    <field name="total_quantity_sold"  />
                    <field name="total_sale_cost"  />
                    <field name="accumulated_sale_cost_percentage"  />
                    <field name="abc"  />
                    <field name="variability_percentage"  />
                    <field name="xyz"  />
                    <field name="nine_box"  />
                    <field name="average_quantity"  />
                    <field name="cost_per_unit"  />
                    <field name="inventory_average_cost"  />
                    <field name="stock_days"  />
                </tree>
            </field>
    </record>

    <record id="action_total_nine_box" model="ir.actions.act_window">
            <field name="name">Nine Box</field>
            <field name="res_model">total.nine.box</field>
            <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_data_wave" name="DataWave" />

    <menuitem id="menu_total_nine_box" name="Nine Box"
              parent="datawave_jz.menu_data_wave"
              action="action_total_nine_box"
             />

</odoo>