<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="form_res_company" model="ir.ui.view">
            <field name="name">res.company.form</field>
            <field name="model">res.company</field>
            <field name="arch" type="xml">
                <form string="Company"     >
                    <notebook>

                        <page id="sql_server" name="sql_server" string="SQL SERVER" >
                            <block title="Sql Server Accesss" id="sql_server_acsess" groups="base.group_no_one">
                            <setting >
                                <field name="sql_server_host" placeholder="Host"/>
                            </setting>
                            <setting >
                                <field name="sql_server_user" placeholder="User" />
                            </setting>
                            <setting >
                                <field name="sql_server_password" placeholder="Password" />
                            </setting>
                            <setting >
                                <field name="sql_server_database" placeholder="Database"/>
                            </setting>
                            <setting >
                                <field name="tenant_id" placeholder="Database"/>
                            </setting>

                        </block>

                        </page>


                        <page id="ranges" name="ranges" string="Ranges" >
                            <block title="Range Configs" id="sql_server_acsess">
                            <setting string="ABC" >

                                <div>A</div>
                                <div class="row mt16">
                                    <field name="abc_a_start"  /><field name="abc_a_end"  />
                                </div>


                                <div>B</div>
                                <div class="row mt16">
                                    <field name="abc_b_start"  /><field name="abc_b_end"  />
                                </div>


                                <div>C</div>
                                <div class="row mt16">
                                    <field name="abc_c_start"  /><field name="abc_c_end"  />
                                </div>


                            </setting>

                            <setting string="ABC MC" >

                                <div>A</div>
                                <div class="row mt16">
                                    <field name="abc_a_start_mc"  /><field name="abc_a_end_mc"  />
                                </div>


                                <div>B</div>
                                <div class="row mt16">
                                    <field name="abc_b_start_mc"  /><field name="abc_b_end_mc"  />
                                </div>


                                <div>C</div>
                                <div class="row mt16">
                                    <field name="abc_c_start_mc"  /><field name="abc_c_end_mc"  />
                                </div>


                            </setting>


                            <setting string="XYZ" >

                                <div>X</div>
                                <div class="row mt16">
                                    <field name="xyz_x_start"  /><field name="xyz_x_end"  />
                                </div>


                                <div>Y</div>
                                <div class="row mt16">
                                    <field name="xyz_y_start"  /><field name="xyz_y_end"  />
                                </div>


                                <div>Z</div>
                                <div class="row mt16">
                                    <field name="xyz_z_start"  /><field name="xyz_z_end"  />
                                </div>


                            </setting>

                            <setting string="XYZ MC" >

                                <div>X</div>
                                <div class="row mt16">
                                    <field name="xyz_x_start_mc"  /><field name="xyz_x_end_mc"  />
                                </div>


                                <div>Y</div>
                                <div class="row mt16">
                                    <field name="xyz_y_start_mc"  /><field name="xyz_y_end_mc"  />
                                </div>


                                <div>Z</div>
                                <div class="row mt16">
                                    <field name="xyz_z_start_mc"  /><field name="xyz_z_end_mc"  />
                                </div>


                            </setting>


                        </block>
                        </page>

                        <page  name="settings_total_nine_box" string="Total Nine Box" >
                            <block title="Total Nine Box" id="settings_total_nine_box">
                            <setting >
                                <field name="nine_box_start_date" placeholder="Start Date"/>
                            </setting>
                            <setting >
                                <field name="nine_box_end_date" placeholder="End Date" />
                            </setting>
                            <setting >
                                 <field name="nine_box_days_per_month" />
                            </setting>
                            <setting >
                                <field name="nine_box_type"/>
                            </setting>

                            <button type="object" name="sync_nine_box" string="Sincronizar"
                                     icon="oi-arrow-right" class="btn-primary"/>

                        </block>

                        </page>

                        <page  name="settings_total_nine_box_mc" string="Total Nine Box MC" >
                            <block title="Total Nine Box MC" id="settings_total_nine_box_mc">
                            <setting >
                                <field name="nine_box_mc_start_date" placeholder="Start Date"/>
                            </setting>
                            <setting >
                                <field name="nine_box_mc_end_date" placeholder="End Date" />
                            </setting>
                            <setting >
                                 <field name="nine_box_mc_days_per_month" />
                            </setting>
                            <setting >
                                <field name="nine_box_mc_type_cost"/>
                            </setting>
                            <setting >
                                <field name="nine_box_mc_type_price"/>
                            </setting>

                            <button type="object" name="sync_nine_box_mc" string="Sincronizar"
                                     icon="oi-arrow-right" class="btn-primary"/>

                        </block>
                        </page>

                        <page  name="settings_total_nine_box" string="Total Nine Box Per Store" >
                            <block title="Total Nine Box Per Store" id="settings_total_nine_box">
                            <setting >
                                <field name="nine_box_per_store_start_date" placeholder="Start Date"/>
                            </setting>
                            <setting >
                                <field name="nine_box_per_store_end_date" placeholder="End Date" />
                            </setting>
                            <setting >
                                 <field name="nine_box_per_store_days_per_month" />
                            </setting>
                            <setting >
                                <field name="nine_box_per_store_type"/>
                            </setting>

                            <button type="object" name="sync_nine_box_per_store" string="Sincronizar"
                                     icon="oi-arrow-right" class="btn-primary"/>

                        </block>
                        </page>

                        <page  name="settings_total_nine_box_mc" string="Total Nine Box Per Store MC" >
                            <block title="Total Nine Box Per Store MC" id="settings_total_nine_box_mc">
                            <setting >
                                <field name="nine_box_mc_per_store_start_date" placeholder="Start Date"/>
                            </setting>
                            <setting >
                                <field name="nine_box_mc_per_store_end_date" placeholder="End Date" />
                            </setting>
                            <setting >
                                 <field name="nine_box_mc_per_store_days_per_month" />
                            </setting>
                            <setting >
                                <field name="nine_box_mc_per_store_type_cost"/>
                            </setting>
                            <setting >
                                <field name="nine_box_mc_per_store_type_price"/>
                            </setting>

                            <button type="object" name="sync_nine_box_per_store_mc" string="Sincronizar"
                                     icon="oi-arrow-right" class="btn-primary"/>

                        </block>

                        </page>



                    </notebook>

                </form>
            </field>
    </record>

    <record id="action_res_company" model="ir.actions.act_window">
            <field name="name">Setting Company</field>
            <field name="res_model">res.company</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('base.view_company_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('form_res_company')})]"
            />

    </record>


    <menuitem id="menu_res_company" name="Settings Company"
              parent="datawave_jz.menu_data_wave"
              action="action_res_company"
              />
</odoo>