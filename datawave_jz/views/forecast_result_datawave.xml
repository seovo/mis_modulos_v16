<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="forecast_result_datawave" model="ir.ui.view">
            <field name="name">forecast.result.datawave.tree</field>
            <field name="model">forecast.result.datawave</field>
            <field name="arch" type="xml">
                <tree string="Service"     >
                    <field name="sku"  />
                    <field name="arima"  />
                    <field name="ets"  />
                    <field name="boosting"  />
                    <field name="prophet"  />
                    <field name="sma"  />
                </tree>
            </field>
    </record>

    <record id="action_forecast_result_datawave" model="ir.actions.act_window">
            <field name="name">Forecast Result Datawave</field>
            <field name="res_model">forecast.result.datawave</field>
            <field name="view_mode">tree</field>
    </record>

    <menuitem id="menu_forecast" name="Forecast" parent="datawave_jz.menu_data_wave" />

    <menuitem id="menu_forecast_result_datawave" name="Nine Box"
              parent="datawave_jz.menu_forecast"
              action="action_forecast_result_datawave"
             />



</odoo>