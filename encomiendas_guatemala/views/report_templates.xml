<?xml version="1.0"?>
<odoo>
    <template id="report_saleorder_document_inherit_sale_stock" inherit_id="sale.report_saleorder_document">
        <xpath expr="//div[@name='expiration_date']" position="after">
            <div class="col-auto col-3 mw-100 mb-2" t-if="doc.carrier_id">
                <strong>Caja Rural:</strong>
                <p  class="m-0"><t t-esc="doc.carrier_id.name" /></p>
            </div>
            <div class="col-auto col-3 mw-100 mb-2" >
                <strong>Peso Cobro:</strong>
                <p  class="m-0"><t t-esc="doc.total_peso_cobro" /></p>
            </div>
        </xpath>
    </template>
</odoo>